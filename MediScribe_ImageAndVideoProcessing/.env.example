# Gemini API Configuration
# Primary API key (required)
GEMINI_API_KEY=your_primary_gemini_api_key_here
GEMINI_MODEL=gemini-2.5-flash
GEMINI_NAME=PRIMARY_GEMINI

# Optional: Additional API keys for rotation/backup
# GEMINI_API_KEY_2=your_secondary_gemini_api_key_here
# GEMINI_MODEL_2=gemini-2.5-flash
# GEMINI_NAME_2=SECONDARY_GEMINI

# GEMINI_API_KEY_3=your_tertiary_gemini_api_key_here
# GEMINI_MODEL_3=gemini-2.5-pro
# GEMINI_NAME_3=TERTIARY_GEMINI

# Training Configuration
DATASET_PATH=./data/images
OUTPUT_MODEL_PATH=./trained_models
BATCH_SIZE=2
LEARNING_RATE=3e-5
NUM_EPOCHS=30

# Hardware Configuration
USE_GPU=true
MIXED_PRECISION=true
GRADIENT_CHECKPOINTING=true